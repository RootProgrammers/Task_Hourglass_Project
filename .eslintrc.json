{
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "commonjs": true
    },
    "extends": [
        "eslint:recommended",
        "react-app",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "sourceType": "module",
        "ecmaFeatures": { "jsx": true }
    },
    "rules": {
        "no-var": "error",
        "no-console": "warn",
        "no-alert": "warn"
    }
}
